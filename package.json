{
	"name": "yan-yun",
	"version": "1.0.0",
	"description": "Репозиторий для разработки frontend-части проекта yan-yun.",
	"scripts": {
		"dev": "next",
		"start": "next start",
		"build": "next build",
		"test": "next test",
		"analyze": "cross-env ANALYZE=true next build",
		"analyze:server": "cross-env BUNDLE_ANALYZE=server next build",
		"analyze:browser": "cross-env BUNDLE_ANALYZE=browser next build",
		"storybook": "start-storybook -p 6006",
		"build-storybook": "build-storybook",
		"gen:api": "openapi-generator-cli generate --skip-validate-spec -i http://localhost:8000/api-json -g typescript-fetch -t ./.openapi-generator/typescript-fetch-swr -o ./generated-api --additional-properties useSingleRequestParameter=false --additional-properties enumPropertyNaming=original && rm -rf ./src/shared/api/back/_generated && mv ./generated-api ./src/shared/api/back/_generated && git add ./src/shared/api/back/_generated && pretty-quick --staged",
		"gen:api-local": "openapi-generator  generate --skip-validate-spec -i http://localhost:8000/api-json -g typescript-fetch -t ./.openapi-generator/typescript-fetch-swr -o ./generated --additional-properties useSingleRequestParameter=false --additional-properties enumPropertyNaming=original && rm -rf src/api/back && cp -r ./generated src/api/back && rm -rf ./generated && npx pretty-quick --staged"
	},
	"husky": {
		"hooks": {
			"pre-commit": "pretty-quick --staged"
		}
	},
